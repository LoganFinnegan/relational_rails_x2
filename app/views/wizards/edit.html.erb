<%= form_with url: "/wizards/#{@wizard.id}", method: :patch do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :level %>
  <%= f.number_field :level %>

  <%= f.label :graduated %>
  <%= f.text_field :graduated %>

  <%= f.label :age %>
  <%= f.number_field :age %>

  <%= f.submit "submit"%>
<% end %>